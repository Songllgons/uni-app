{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": false,
    "declaration": true,
    "declarationDir": "./dist/packages/uni-preprocess/src",
    "target": "ESNext",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": false,
    "lib": ["ESNext"],
    "types": ["node"]
  },
  "include": ["src"]
}
